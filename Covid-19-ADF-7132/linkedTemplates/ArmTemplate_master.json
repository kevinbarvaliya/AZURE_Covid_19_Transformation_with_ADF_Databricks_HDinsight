{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "Covid-19-ADF-7132"
		},
		"ls_ablob_covid_19_sa_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_ablob_covid_19_sa'"
		},
		"ls_adls_covid_19_dl_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_covid_19_dl'"
		},
		"ls_sql_covid_db_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_sql_covid_db'"
		},
		"ls_adls_covid_19_dl_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://covid19datalake7132.dfs.core.windows.net/"
		},
		"ls_http_git_oendata_ecdc_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().sourceBaseURL}"
		},
		"tr_population_data_arrived_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/b70114fd-1f01-4ae0-a331-3350a98201f0/resourceGroups/Covid-19-project/providers/Microsoft.Storage/storageAccounts/covid19storagaccount7132"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "Covid-19-ADF-7132_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_ablob_covid_19_sa_connectionString": {
						"value": "[parameters('ls_ablob_covid_19_sa_connectionString')]"
					},
					"ls_adls_covid_19_dl_accountKey": {
						"value": "[parameters('ls_adls_covid_19_dl_accountKey')]"
					},
					"ls_sql_covid_db_connectionString": {
						"value": "[parameters('ls_sql_covid_db_connectionString')]"
					},
					"ls_adls_covid_19_dl_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_covid_19_dl_properties_typeProperties_url')]"
					},
					"ls_http_git_oendata_ecdc_properties_typeProperties_url": {
						"value": "[parameters('ls_http_git_oendata_ecdc_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "Covid-19-ADF-7132_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"tr_population_data_arrived_properties_typeProperties_scope": {
						"value": "[parameters('tr_population_data_arrived_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'Covid-19-ADF-7132_ArmTemplate_0')]"
			]
		}
	]
}